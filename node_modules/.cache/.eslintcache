n            </div>\r\n            <div>\r\n                <Paginate\r\n                    videogamesPerPage={videogamesPerPage}\r\n                    videogames={videogames.length}\r\n                    paginate={paginate}\r\n                    currentPage={currentPage}\r\n                />\r\n            </div>\r\n            <div className={styles.container}>\r\n                {\r\n                    currentVideogames.length ? (\r\n                        currentVideogames.map((game, index) => {\r\n                            return (\r\n                                <Card\r\n                                    key={index}\r\n                                    id={game.id}\r\n                                    name={game.name}\r\n                                    image={game.background_image}\r\n                                    genres={game.genres.join(\", \")}\r\n                                />\r\n                            )\r\n                        })\r\n                    ) : <h1 className={styles.loading}>LOADING...</h1>\r\n                }\r\n            </div>\r\n        </div>\r\n    )\r\n}","C:\\Users\\Gonzalo\\Desktop\\PI-Videogames-main\\PI-Videogames-main\\client\\src\\components\\Detail\\Detail.jsx",[],"C:\\Users\\Gonzalo\\Desktop\\PI-Videogames-main\\PI-Videogames-main\\client\\src\\components\\Form\\Form.jsx",[],"C:\\Users\\Gonzalo\\Desktop\\PI-Videogames-main\\PI-Videogames-main\\client\\src\\redux\\reducer.js",[],"C:\\Users\\Gonzalo\\Desktop\\PI-Videogames-main\\PI-Videogames-main\\client\\src\\redux\\actionTypes.js",[],"C:\\Users\\Gonzalo\\Desktop\\PI-Videogames-main\\PI-Videogames-main\\client\\src\\components\\SearchBar\\SearchBar.jsx",[],"C:\\Users\\Gonzalo\\Desktop\\PI-Videogames-main\\PI-Videogames-main\\client\\src\\components\\Card\\Card.jsx",[],"C:\\Users\\Gonzalo\\Desktop\\PI-Videogames-main\\PI-Videogames-main\\client\\src\\redux\\actions.js",[],"C:\\Users\\Gonzalo\\Desktop\\PI-Videogames-main\\PI-Videogames-ma